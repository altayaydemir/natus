{
  "name": "natus",
  "private": true,
  "version": "1.0.0",
  "description": "A basic frontend for Put.io API",
  "main": "src/index.jsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development node config/devServer.js",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/prod.js"
  },
  "keywords": [
    "react",
    "redux",
    "put.io"
  ],
  "author": "Altay Aydemir <altay.aydemir@gmail.com>",
  "repository": {
    "url": "https://github.com/altayaydemir/natus.git",
    "type": "git"
  },
  "homepage": "https://github.com/altayaydemir/natus",
  "bugs": {
    "url": "https://github.com/altayaydemir/natus/issues"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "classnames": "2.2.5",
    "font-awesome": "^4.7.0",
    "react": "15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "15.4.1",
    "react-fontawesome": "^1.5.0",
    "react-helmet": "3.2.3",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "4.0.7",
    "react-router-scroll": "0.4.1",
    "react-select": "^1.0.0-rc.2",
    "redux": "3.6.0",
    "redux-form": "6.3.2",
    "redux-thunk": "2.1.0",
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.9",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "eslint": "3.12.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-webpack": "0.7.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "json-loader": "0.5.4",
    "lodash.merge": "4.6.0",
    "node-sass": "3.13.1",
    "postcss-loader": "1.2.0",
    "progress-bar-webpack-plugin": "1.9.0",
    "react-hot-loader": "3.0.0-beta.6",
    "reduce-reducers": "0.1.2",
    "redux-devtools-extension": "1.0.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "validator": "^6.2.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "global-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/first": 0,
      "import/extensions": 0,
      "import/no-dynamic-require": 0,
      "no-shadow": 0,
      "camelcase": 0,
      "no-confusing-arrow": 0,
      "no-return-assign": 0,
      "no-nested-ternany": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/no-unused-prop-types": 0,
      "react/no-danger": 0,
      "react/self-closing-comp": 0,
      "wrap-iife": 0,
      "no-eval": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./config/webpack/base.js"
        }
      }
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-object-rest-spread",
      "syntax-async-functions",
      "transform-async-to-generator"
    ]
  }
}
